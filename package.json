{
  "name": "hobbies",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/nest-backend",
    "apps/backend",
    "apps/frontend"
  ],
  "scripts": {
    "dev": "npm run dev --workspace=apps/backend",
    "dev:backend": "npm run dev --workspace=apps/backend",
    "dev:frontend": "npm run dev --workspace=apps/frontend",
    "build:backend": "npm run build --workspace=apps/backend",
    "build:frontend": "npm run build --workspace=apps/frontend",
    "prisma:generate": "npx prisma generate --config apps/backend/prisma/prisma.config.ts",
    "prisma:migrate:create": "npx prisma migrate dev --create-only --config apps/backend/prisma/prisma.config.ts",
    "prisma:migrate": "npx prisma migrate dev --config apps/backend/prisma/prisma.config.ts",
    "prisma:reset": "npx prisma migrate reset --config apps/backend/prisma/prisma.config.ts",
    "prisma:studio": "npx prisma studio --config apps/backend/prisma/prisma.config.ts"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "react-router": "^7.13.0"
  }
}
